        .navbar-nav .dropdown:hover .dropdown-menu,
        .navbar-nav .dropdown.show .dropdown-menu {
            display: block;
        }

        /* Tùy chỉnh kiểu dáng dropdown */
        .dropdown-menu {
            display: none;
            position: absolute;
            background-color: #fff; /* Màu nền của dropdown */
            box-shadow: 0 8px 16px rgba(0,0,0,0.1); /* Đổ bóng */
            z-index: 1;
        }

            .dropdown-menu a {
                color: #333; /* Màu chữ của mục dropdown */
                padding: 12px 16px;
                text-decoration: none;
                display: block;
            }

                .dropdown-menu a:hover {
                    background-color: #f2f2f2; /* Màu nền khi hover */
                }

        /* Thêm biểu tượng mũi tên bên ngoài */
