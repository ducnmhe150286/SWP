<!-- Featured products -->
<div class="small-container">
  <h2 class="title">Sản Phẩm Chào Bán</h2>
            

<form action="@Url.Action("Index", "Home")" method="get" class="mt-3">
           
            <div class="row mb-3 mt-5 mb-5">
                <div class="">
                    <label for="categoryFilter" class="form-label">Danh mục:</label>
                    <select id="categoryFilter" name="categoryFilter" class="">
                        <option value="">Tất cả</option>
                        @foreach (var category in ViewBag.Categories)
                        {
                            <option value="@category.CategoryId" selected="@(category.CategoryId == ViewBag.CategoryFilter)">@category.CategoryName</option>
                       }
                    </select>
                </div>
                <div class="">
                    <label for="brandFilter" class="form-label">Thương hiệu:</label>
                    <select id="brandFilter" name="brandFilter" class="">
                        <option value="">Tất cả</option>
                        @foreach (var brand in ViewBag.Brands)
                        {
                            <option value="@brand.BrandId" selected="@(brand.BrandId == ViewBag.brandFilter)">@brand.BrandName</option>
                        }
                    </select>
                </div>
                <div class="">
                    @*                <label for="searchString" class="form-label">Tìm kiếm:</label>
                    *@                <div class="row g-2 align-items-center">
                        <div class="col-8">
        <input type="text" id="searchString" name="searchString" class="" placeholder="Nhập từ khóa..." value="@ViewData["SearchString"]">
                        </div>
                        <div class="col-1">
                            <button type="submit" class="mt-1 btn btn-primary btn-sm" onclick="handleSearch()">Tìm kiếm</button>
                        </div>
                    </div>
                </div>
            </div>

        </form>
       <div class="d-flex justify-content-center">
    <h2 class="mess" style="color: red; text-align: center;">@ViewData["ErrorMessage"]</h2>
    <h2 class="mess" style="color: green; text-align: center;">@ViewData["SuccessMessage"]</h2>
</div>

            
        <div class="row" style="padding-top: 30px;" id="product">
            @foreach (Product product in ViewBag.ProductList)
            {
                <div class="col-4 product-card">
                    @if (product.ProductImages != null && product.ProductImages.Any())
                    {
                        <a asp-controller="ProductDetail" asp-action="Index" asp-route-productId="@product.ProductId">
                            <img src="/Images/@product.ProductImages.First().Path" alt="Image" class="img-thumbnail" width="180" height="240" />
                        </a>
                    }
                    else
                    {
                        <a asp-controller="ProductDetail" asp-action="Index" asp-route-productId="@product.ProductId">
                            <img src="default_image_url" alt="Default Image" class="img-thumbnail" style="max-width: 100px; max-height: 100px;" />
                        </a>
                    }

                    <a asp-controller="ProductDetail" asp-action="Index" asp-route-productId="@product.ProductId">
                        <h4>@product.ProductName</h4>
                    </a>

                    <!-- Your other code here -->

                    <p>@string.Format("{0:N0}", product.Price)₫</p>
                    <!-- Add "Buy Now" button -->
                    <a asp-controller="ProductDetail" asp-action="Index" asp-route-productId="@product.ProductId" class="btn1-buy-now">Buy Now</a>

                </div>
            }
        </div>
<div class="mt-3 d-flex justify-content-center">
            <nav class="text-center" aria-label="Page navigation">
                <ul class="pagination">
                    @if (ViewBag.ProductList.HasPreviousPage)
                    {
                        <li class="page-item">
                            <a class="page-link" href="@Url.Action("Index", new { page = ViewBag.ProductList.PageIndex - 1 })" aria-label="Previous">
                                <span aria-hidden="true">&laquo;</span>
                            </a>
                        </li>
                    }
                    @for (int i = 1; i <= ViewBag.ProductList.TotalPages; i++)
                    {
                        <li class="page-item @(i == ViewBag.ProductList.PageIndex ? "current-page" : "")">
                            <a class="page-link" href="@Url.Action("Index", new { page = i })">@i</a>
                        </li>
                    }
                    @if (ViewBag.ProductList.HasNextPage)
                    {
                        <li class="page-item">
                            <a class="page-link" href="@Url.Action("Index", new { page = ViewBag.ProductList.PageIndex + 1 })" aria-label="Next">
                                <span aria-hidden="true">&raquo;</span>
                            </a>
                        </li>
                    }
                </ul>
            </nav>
        </div>






