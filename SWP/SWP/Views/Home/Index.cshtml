    <script>
        // Paste your JavaScript code here
        var MenuItems = document.getElementById('MenuItems');
        MenuItems.style.maxHeight = '0px';

        function menutoggle() {
            if (MenuItems.style.maxHeight == '0px') {
                MenuItems.style.maxHeight = '200px';
            } else {
                MenuItems.style.maxHeight = '0px';
            }
        }

        var queryString = window.location.search;
        var urlParams = new URLSearchParams(queryString);

        // Nếu có tham số truy vấn và có giá trị "searchString"
        if (urlParams.has('searchString')) {
            // Lấy phần tử có id là "product"
            var productElement = document.getElementById("product");

            // Nếu phần tử tồn tại
            if (productElement) {
                // Cuộn trang đến vị trí của phần tử "product"
                productElement.scrollIntoView({ behavior: "smooth" });
            }
        }
        function handleSearch() {
            var searchStringInput = document.getElementById("searchString");
            var currentUrl = new URL(window.location.href);

            // Nếu ô tìm kiếm có giá trị
            if (searchStringInput && searchStringInput.value.trim() !== "") {
                // Thêm tham số truy vấn vào URL
                currentUrl.searchParams.set("searchString", searchStringInput.value.trim());
            } else {
                // Nếu ô tìm kiếm trống, xóa tham số truy vấn từ URL
                currentUrl.searchParams.delete("searchString");
            }

            // Chuyển hướng đến URL đã cập nhật
            window.location.href = currentUrl.href;
        }
    </script>

